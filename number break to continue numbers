
a =input("请输入一个数a：");
while True:
    if not a.isdigit():
        print ("Error:try again!")
        a = input("请重新输入:");
    else:
        break;
a = int(a);
b = int(a/2)+1;
for i in range(1,b+1):
    sum = 0;
    for m in range(i,b+1):
        sum = sum+m;
        if(sum ==a):
            for x in range(i,m+1):
                print(x,end=" ")
print()
print("game over")

